{
  "name": "auto-puppeteer",
  "version": "1.0.0",
  "main": "./src/main.mjs",
  "scripts": {
    "start": "node src/main.mjs",
    "copyFile": "node script/copy.js",
    "build": "rimraf ./dist && rollup -c",
    "build:pkg": "npm run build &&rimraf ./build && pkg . && npm run copyFile",
    "build:pkg2": "pkg ./test/test.js --target node18-macos-arm64  --output ./build2"
  },
  "type": "module",
  "bin": "./dist/main.js",
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": [
      "assets/**/*",
      "package.json",
      "config.json"
    ],
    "targets": [
      "node18-macos-arm64",
      "node18-macos-x64",
      "node18-win-x64"
    ],
    "outputPath": "./build"
  },
  "engines": {
    "node": ">= 18"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "dayjs": "^1.11.13",
    "node-fetch": "^3.3.2",
    "node-xlsx": "^0.24.0",
    "puppeteer": "^23.5.1",
    "puppeteer-core": "^23.5.1"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@types/node": "^22.7.5",
    "pkg": "^5.8.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.24.0",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
